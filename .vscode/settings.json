    "**/.gitignore": true,
    "**/.idea": true,
    "**/.settings/**": true,
    "**/.tmp": true,
    "**/fontconfig": true,
    "**/LICENSE": true,
    "**/logs": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    "**/.angular/*": true,
    ".angular": true,
    ".wrangler": true,
    ".yarn": true,
    "node_modules": true,
    "dist": true,
    ".yarnrc.yml": true,
    ".eslintcache": true,
    ".dockerignore": true,
    "**/.eslintrc.json": false,
    ".ci": false,
    ".github": false,
    ".gitlab-ci.yml": false,
    "decorate-angular-cli.js": true,
    ".editorconfig": true,
    ".gitignore": true,
    "**/.gitkeep": true,
