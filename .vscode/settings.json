    "apps/angular-notary-room/src/manifest.webmanifest": true
  },
  "files.watcherExclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
