{
  "[javascript]": {
    "editor.foldingStrategy": "indentation",
  },
  "eslint.enable": true,
  "eslint.format.enable": true,
  "todohighlight.keywords": [
    {
      "text": "TODO:",
      "isWholeLine": true,
      "color": "red",
      "border": "1px solid red",
      "borderRadius": "2px",
      //NOTE: using borderRadius along with `border` or you will see nothing change
      "backgroundColor": "rgba(0,0,0,.2)",
      //other styling properties goes here ... 
    },
    {
      "text": "NOTE:",
      "isWholeLine": true,
      "color": "rgb(255,199,0)",
      "border": "1px solid rgb(255,199,0)",
      "borderRadius": "2px",
      "backgroundColor": "rgba(0,0,0,.2)",
      "overviewRulerColor": "grey"
    },
    {
      "text": "DESC:",
      "isWholeLine": true,
      "color": "rgb(146,203,133)",
      "border": "1px solid rgb(146,203,133)",
      "borderRadius": "2px",
      "backgroundColor": "rgba(0,0,0,.2)",
      "overviewRulerColor": "grey"
    }
  ],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
